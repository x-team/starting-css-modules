{
  "name": "starting-css-modules",
  "version": "1.0.0",
  "description": "A very simple example of css-modules",
  "scripts": {
    "clean": "rm -fR dist",
    "copy": "cp -R src dist",
    "prebuild": "npm run clean && npm run copy",
    "build": "monobrow -c monobrow/config.js",
    "start": "npm run build && ecstatic dist",
    "lint": "standard"
  },
  "author": "joshwnj",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "css-modulesify": "0.26.0",
    "ecstatic": "^2.1.0",
    "monobrow": "2.1.0",
    "standard": "^4.5.4"
  },
  "dependencies": {
    "autoprefixer": "^6.6.0",
    "classnames": "^2.2.5",
    "flexboxgrid": "^6.3.1"
  }
}
